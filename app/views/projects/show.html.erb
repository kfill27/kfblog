<div class="container">
  <p id="notice"><%= notice %></p>
  <div class="col-md-6 col-md-offset-3">
    <%= image_tag @project.project_image.url %>
    <%= @project.name %>
    <%= @project.url %>
    <%= @project.description %>
  </div>

  <div class="buttons">
    <%= link_to 'Edit', edit_project_path(@project) %> |
    <%= link_to 'Back', projects_path %>
  </div>
</div>
